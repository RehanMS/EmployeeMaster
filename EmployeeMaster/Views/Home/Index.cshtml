@model List<EmployeeMaster.Models.MultiData>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link href="~/lib/simplePagination.js/simplePagination.css" rel="stylesheet" />
    <script src="~/lib/simplePagination.js/jquery.simplePagination.js"></script>
    <script src="~/scripts/site.js"></script>
</head>
<body>
    <div class="d-flex justify-content-center min-vh-100">
        <form action="" method="post" class=" border-info rounded-3 alert alert-success w-100">
            <h1 class=" alert alert-warning text-center border-0">EmployeeMaster Information</h1>
            <a href="/Home/Create" class="btn btn-outline-success mt-3" style="margin-bottom:2%">Create</a>

            <table class="table table-striped">
                <tr>
                    <th>Employee Id</th>
                    <th>Employee No</th>
                    <th>Employee Name</th>
                    <th>Email</th>
                    <th>Mobile</th>
                    <th>Address</th>
                    <th>Designation Id</th>
                    <th>Designation Name</th>
                    <th>Actions</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.employeeMasters.EmployeeId</td>
                        <td>@item.employeeMasters.EmployeeName</td>
                        <td>@item.employeeMasters.EmployeeNo</td>
                        <td>@item.employeeMasters.Email</td>
                        <td>@item.employeeMasters.Mobile</td>
                        <td>@item.employeeMasters.Address</td>
                        <td>@item.designationMasters.DesignationId</td>
                        <td>@item.designationMasters.DesignationName</td>
                        <td>
                            <a class="btn btn-outline-info" href="/Home/Details/@item.employeeMasters.EmployeeId">Details</a>
                            <a class="btn btn-outline-warning" href="/Home/Edit/@item.employeeMasters.EmployeeId">Edit</a>
                            <a class="btn btn-outline-danger" href="/Home/Delete/@item.employeeMasters.EmployeeId">Delete</a>
                        </td>
                    </tr>

                }
            </table>
        </form>
    </div>
</body>
</html>
